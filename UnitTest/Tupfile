
include_rules

LIBS += -lboost_unit_test_framework
INCLUDE_DIRS += -isystem$(TURTLE_DIR)/include

: foreach *.cpp |> !cxx |>
: ./*.o $(SOURCE_DIR)/*.o $(DECISION_TREE_DIR)/*.o $(DUMPER_DIR)/*.o \
		$(STATUS_DIR)/*.o $(CPP_UTIL_LIB) |> !linker |> unitTest

export LD_LIBRARY_PATH

: unitTest |> !unitTest |>
